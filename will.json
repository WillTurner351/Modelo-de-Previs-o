      "regularization": 0.01,
      "learning_rate": 0.001,
      "epochs": 100
    }
  },
  "exemplo_dados": {
    "data_inicio": "2020-01-01",
    "data_fim": "2023-12-31",
    "num_amostras": 10000,
    "num_features": 3
  }
}
