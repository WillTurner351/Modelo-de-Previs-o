{
  "nome": "Modelo de Previsão de Vendas",
  "descricao": "Um modelo de previsão simples para prever vendas futuras com base em dados históricos.",
  "versao": "1.0",
  "pontos_extremidade": {
    "treinamento": "http://api.example.com/train",
    "previsao": "http://api.example.com/predict",
    "avaliacao": "http://api.example.com/evaluate"
  },
  "parametros": {
    "algoritmo": "Regressão Linear",
    "features": ["tempo", "preço", "promoção"],
    "target": "vendas",
